<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.myfirebase">

    <uses-permission android:name="android.permission.INTERNET" />
    <uses-permission android:name="android.permission.CAMERA" />

    <application
        android:name=".Application.MyFirebaseApp"
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".Activity.DetalhesUsuariosActivity"></activity>
        <activity android:name=".Activity.DashboardActivity" />
        <activity android:name=".Activity.SplashActivity" />
        <activity
            android:name=".Activity.DetalhesActivity"
            android:parentActivityName=".Activity.ListarPessoaActivity" />
        <activity
            android:name=".Activity.ListarPessoaActivity"
            android:parentActivityName=".Activity.PerfilActivity" />
        <activity
            android:name=".Activity.CadastroPessoaActivity"
            android:parentActivityName=".Activity.ListarPessoaActivity" />
        <activity android:name=".Activity.ResetSenhaActivity" />
        <activity android:name=".Activity.PerfilActivity" />
        <activity
            android:name=".Activity.CadastroUserActivity"
            android:parentActivityName=".Activity.LoginActivity" />
        <activity android:name=".Activity.MainActivity" />
        <activity android:name=".Activity.LoginActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>